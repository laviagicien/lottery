    "angular": "^1.7.9",
    "bootstrap": "^4.4.1",
    "browserfs": "^1.4.3",
    "darkmode-js": "^1.5.7",
    "electron-is-dev": "^1.2.0",
    "file-system": "^2.2.2",
    "global": "^4.4.0",
    "is-electron": "^2.2.0",
    "jquery": "^3.5.1",
    "knex": "^0.21.5",
